class Solution(object):
    def isPalindrome(self,x):
        if x<0 or (x%10==0 and x!=0):
            return False
        palindrome=0
        while x>palindrome:
            palindrome=palindrome*10+x%10
            x//=10
        return x== palindrome or x== palindrome//10
sol=Solution()
x=121
print("input",x)
print("is palindrome:",sol.isPalindrome(x))        
